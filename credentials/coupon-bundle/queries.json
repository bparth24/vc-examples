{
  "default": {
    "type": "QueryByExample",
    "credentialQuery": [{
      "reason": "Please present your CouponBundleCredential or Verifiable Credential(s) to access available discounts and offers.",
      "example": {
        "@context": [
          "https://www.w3.org/ns/credentials/v2",
          "https://examples.vcplayground.org/contexts/coupon-bundle/v2.json"
        ],
        "type": ["CouponBundleCredential"]
      },
      "acceptedCryptosuites": [
        "Ed25519Signature2020",
        "eddsa-rdfc-2022",
        "ecdsa-rdfc-2019",
        "bbs-2023",
        "ecdsa-sd-2023"
      ]
    }]
  },
  "redemption": {
    "type": "QueryByExample",
    "credentialQuery": [{
      "reason": "Please present your CouponBundleCredential to redeem coupons for this transaction.",
      "example": {
        "@context": [
          "https://www.w3.org/ns/credentials/v2",
          "https://examples.vcplayground.org/contexts/coupon-bundle/v2.json"
        ],
        "type": ["CouponBundleCredential"],
        "credentialSubject": {
          "bundleMatcher": "https://couponbureau.example/matcher"
        }
      },
      "acceptedCryptosuites": [
        "Ed25519Signature2020",
        "eddsa-rdfc-2022",
        "ecdsa-rdfc-2019",
        "bbs-2023",
        "ecdsa-sd-2023"
      ]
    }]
  }
}
